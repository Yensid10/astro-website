---
import { getImage, Image } from 'astro:assets'
import Section from '@components/team/Section.astro'

const { name } = Astro.props;
---
<Section bgColor="yellow" className="py-20" {...Astro.props}>
  <div class="flex flex-col">
    <h3 class="text-xl font-medium">GitHub Repositories</h3>
    <p class="pb-10">{name} is a key contributor to the following libraries</p>
  </div>
  <slot/>
</Section>
